// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://"
}

// cria uma tabela
model Feedback {
  id String @id @default(uuid()) //primary key, unique

  type       String
  comment    String
  screenShot String? // opcional

  @@map("feedbacks")
}
